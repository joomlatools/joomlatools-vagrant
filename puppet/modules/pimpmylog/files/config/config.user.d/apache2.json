{
  "apache-main-error-log": {
    "display": "[apache] main error log",
    "path": "/var/log/apache2/error.log",
    "tags" : ["Apache (errors)"],
    "refresh": 5,
    "max": 20,
    "notify": true,
    "multiline": "",
    "format": {
      "regex": "|^\\[(.*)\\] \\[(.*)\\] (\\[client (.*)\\] )*((?!\\[client ).*)(, referer: (.*))*$|U",
      "export_title": "Log",
      "match": {
        "Date": 1,
        "IP": 4,
        "Log": 5,
        "Severity": 2,
        "Referer": 7
      },
      "types": {
        "Date": "date:H:i:s",
        "IP": "ip:http",
        "Log": "pre",
        "Severity": "badge:severity",
        "Referer": "link"
      },
      "exclude": {
        "Log": [
          "\/PHP Stack trace:\/",
          "\/PHP *[0-9]*\\. \/"
        ]
      }
    }
  },
  "apache-vhost-access-logs": {
    "display": "[apache]",
    "path": "/var/log/apache2/*-access_log",
    "tags" : ["Apache (access)"],
    "count": 50,
    "refresh": 5,
    "max": 20,
    "notify": false,
    "multiline": "",
    "format": {
      "regex": " |^(.*) (.*) (.*) \\[(.*)\\] \"(.*) (.*) (.*)\" ([0-9]*) (.*) \"(.*)\" \"(.*)\"( [0-9]*\/([0-9]*))*$|U",
      "export_title": "URL",
      "match": {
        "Date": 4,
        "IP": 1,
        "CMD": 5,
        "URL": 6,
        "Code": 8,
        "Size": 9,
        "Referer": 10,
        "UA": 11,
        "User": 3,
        "\u03bcs": 13
      },
      "types": {
        "Date": "date:H:i:s",
        "IP": "ip:geo",
        "URL": "txt",
        "Code": "badge:http",
        "Size": "numeral:0b",
        "Referer": "link",
        "UA": "ua:{os.name} {os.version} | {browser.name} {browser.version}\/100",
        "\u03bcs": "numeral:0,0"
      },
      "exclude": {
        "URL": [
          "\/favicon.ico\/",
          "\/\\.pml\\.php.*$\/"
        ],
        "CMD": ["\/OPTIONS\/"]
      }
    }
  },
  "apache-vhost-error-logs": {
    "display": "[apache]",
    "path": "/var/log/apache2/*-error_log",
    "tags" : ["Apache (errors)"],
    "count": 50,
    "refresh": 5,
    "max": 20,
    "notify": true,
    "multiline": "",
    "format": {
      "regex": "|^\\[(.*)\\] \\[(.*)\\] (\\[client (.*)\\] )*((?!\\[client ).*)(, referer: (.*))*$|U",
      "export_title": "Log",
      "match": {
        "Date": 1,
        "IP": 4,
        "Log": 5,
        "Severity": 2,
        "Referer": 7
      },
      "types": {
        "Date": "date:H:i:s",
        "IP": "ip:http",
        "Log": "pre",
        "Severity": "badge:severity",
        "Referer": "link"
      },
      "exclude": {
        "Log": [
          "\/PHP Stack trace:\/",
          "\/PHP *[0-9]*\\. \/"
        ]
      }
    }
  }
}